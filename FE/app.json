{
  "expo": {
    "name": "AwesomeProjectUI",
    "slug": "AwesomeProjectUI",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "plugins": [
      [
        "expo-av",
        {
          "microphonePermission": "Allow AwesomeProjectUI to access your microphone."
        }
      ],
      ["@react-native-google-signin/google-signin"],
      ["react-native-fbsdk-next",
      {
        "appID": "571327921863104",
        "clientToken": "124031ad3c5823165463a4298d521388",
        "displayName": "reactnative_ui",
        "advertiserIDCollectionEnabled": false,
        "autoLogAppEventsEnabled": false,
        "isAutoInitEnabled": true,
        "iosUserTrackingPermission": "This identifier will be used to deliver personalized ads to you."
      }]
    ],
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "SKAdNetworkItems": [
          {
            "SKAdNetworkIdentifier": "v9wttpbfk9.skadnetwork"
          },
          {
            "SKAdNetworkIdentifier": "n38lu8286q.skadnetwork"
          }
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#F0EFEF"
      },
      "permissions": [
        "com.google.android.gms.permission.ACTIVITY_RECOGNITION",
        "ACTIVITY_RECOGNITION",
        "VIBRATE",
        "android.permission.RECORD_AUDIO",
        "android.permission.MODIFY_AUDIO_SETTINGS",
        "android.permission.INTERNET"
      ],
      "googleServicesFile": "./google-services.json",
      "package": "com.Ui.healthy_app123"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "3f4bc570-0daa-4d45-9745-0c99b51329bf"
      }
    }
  }
}
